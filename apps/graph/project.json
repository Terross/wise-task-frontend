{
  "name": "graph",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/graph/src",
  "projectType": "application",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/graph"
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/graph/src/environments/environment.ts",
              "with": "apps/graph/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "vite build --watch",
        "cwd": "apps/graph"
      }
    },
    "dev": {
      "executor": "@nx/vite:serve",
      "options": {
        "port": 4200
      }
    },
    "preview": {
      "executor": "@nx/vite:preview",
      "options": {
        "port": 4200
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/graph/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  }
}
